

if (!process.nextTick) {
  process.nextTick = function (callback) {
    for (var _len = arguments.length, args = Array(_len > 1 ? _len - 1 : 0), _key = 1; _key < _len; _key++) {
      args[_key - 1] = arguments[_key];
    }

    setTimeout(function () {
      return callback.apply(undefined, args);
    }, 0);
  };
}